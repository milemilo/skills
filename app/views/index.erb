<div class="container">
  <h1>User Skills and Levels</h1>

  <%# TODO: display the skills and proficiency levels for each user ... %>
  <ul>
  <% @users.each do |user| %>
    <li><%= user.name %></li>
      <% user.skills.each do |skill| %>
      	<ul>
      	 <li>Skill Name: <%= skill.name %> Formal Training? <%= skill.proficiencies.first.formal %> Years Experience: <%= skill.proficiencies.first.years %></li>
      	</ul>

      <% end %>
    <% end %>
  </ul>
</div>
